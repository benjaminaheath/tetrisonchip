#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision + 0;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
S_0x55f8c072b3a0 .scope module, "test_lfsr" "test_lfsr" 2 1;
 .timescale 0 0;
v0x55f8c073fa20_0 .var "clk", 0 0;
v0x55f8c073fac0_0 .var "nreset", 0 0;
v0x55f8c073fb90_0 .net "out", 14 0, v0x55f8c073f740_0;  1 drivers
S_0x55f8c072b530 .scope module, "lfsr15" "lfsr" 2 6, 3 1 0, S_0x55f8c072b3a0;
 .timescale 0 0;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "nreset";
    .port_info 2 /OUTPUT 15 "out";
L_0x55f8c073fe00 .functor XOR 1, L_0x55f8c073fc90, L_0x55f8c073fd60, C4<0>, C4<0>;
L_0x55f8c07400b0 .functor XOR 1, L_0x55f8c073ff40, L_0x55f8c073ffe0, C4<0>, C4<0>;
v0x55f8c072b6c0_0 .net *"_ivl_1", 0 0, L_0x55f8c073fc90;  1 drivers
v0x55f8c073f2f0_0 .net *"_ivl_3", 0 0, L_0x55f8c073fd60;  1 drivers
v0x55f8c073f3d0_0 .net *"_ivl_7", 0 0, L_0x55f8c073ff40;  1 drivers
v0x55f8c073f490_0 .net *"_ivl_9", 0 0, L_0x55f8c073ffe0;  1 drivers
v0x55f8c073f570_0 .net "clk", 0 0, v0x55f8c073fa20_0;  1 drivers
v0x55f8c073f680_0 .net "nreset", 0 0, v0x55f8c073fac0_0;  1 drivers
v0x55f8c073f740_0 .var "out", 14 0;
v0x55f8c073f820_0 .net "tap1", 0 0, L_0x55f8c073fe00;  1 drivers
v0x55f8c073f8e0_0 .net "tap2", 0 0, L_0x55f8c07400b0;  1 drivers
E_0x55f8c0729ba0/0 .event negedge, v0x55f8c073f680_0;
E_0x55f8c0729ba0/1 .event posedge, v0x55f8c073f570_0;
E_0x55f8c0729ba0 .event/or E_0x55f8c0729ba0/0, E_0x55f8c0729ba0/1;
L_0x55f8c073fc90 .part v0x55f8c073f740_0, 0, 1;
L_0x55f8c073fd60 .part v0x55f8c073f740_0, 13, 1;
L_0x55f8c073ff40 .part v0x55f8c073f740_0, 0, 1;
L_0x55f8c073ffe0 .part v0x55f8c073f740_0, 14, 1;
    .scope S_0x55f8c072b530;
T_0 ;
    %wait E_0x55f8c0729ba0;
    %load/vec4 v0x55f8c073f680_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 1, 0, 15;
    %assign/vec4 v0x55f8c073f740_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x55f8c073f740_0;
    %parti/s 13, 0, 2;
    %load/vec4 v0x55f8c073f820_0;
    %concat/vec4; draw_concat_vec4
    %load/vec4 v0x55f8c073f8e0_0;
    %concat/vec4; draw_concat_vec4
    %assign/vec4 v0x55f8c073f740_0, 0;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x55f8c072b3a0;
T_1 ;
    %delay 10, 0;
    %load/vec4 v0x55f8c073fa20_0;
    %inv;
    %store/vec4 v0x55f8c073fa20_0, 0, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x55f8c072b3a0;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f8c073fa20_0, 0, 1;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f8c073fac0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55f8c073fac0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55f8c073fac0_0, 0, 1;
    %delay 200, 0;
    %vpi_call 2 25 "$finish" {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x55f8c072b3a0;
T_3 ;
    %vpi_call 2 30 "$monitor", "Time: %0t | nReset: %b | LFSR Output: %b", $time, v0x55f8c073fac0_0, v0x55f8c073fb90_0 {0 0 0};
    %end;
    .thread T_3;
# The file index is used to find the file name in the following table.
